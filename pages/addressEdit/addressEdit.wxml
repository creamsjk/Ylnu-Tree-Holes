<view><view class="address_edit"><view class="divContent"><view class="divItem"><text style="font-weight:bold;">联系人：</text><view class="inputUser"><u-input bind:input="__e" vue-id="67d6d6c2-1" fontSize="28rpx" border="none" maxlength="10" placeholder="请填写收货人的姓名" value="{{form.consignee}}" data-event-opts="{{[['^input',[['__set_model',['$0','consignee','$event',[]],['form']]]]]}}" bind:__l="__l"></u-input></view><text data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="spanChecked" bindtap="__e"><text class="{{[(form.gender==='1')?'iActive':'']}}"></text>先生</text><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="spanChecked" bindtap="__e"><text class="{{[(form.gender==='gender0')?'iActive':'']}}"></text>女士</text></view><view class="divItem"><text style="font-weight:bold;">手机号：</text><u-input bind:input="__e" vue-id="67d6d6c2-2" fontSize="28rpx" border="none" maxlength="11" placeholder="请填写收货人手机号码" value="{{form.phone}}" data-event-opts="{{[['^input',[['__set_model',['$0','phone','$event',[]],['form']]]]]}}" bind:__l="__l"></u-input></view><view class="divItem"><text style="font-weight:bold;">收货地址：</text><u-input bind:input="__e" vue-id="67d6d6c2-3" fontSize="28rpx" border="none" maxlength="140" placeholder="请输入收货地址" value="{{form.detail}}" data-event-opts="{{[['^input',[['__set_model',['$0','detail','$event',[]],['form']]]]]}}" bind:__l="__l"></u-input></view><view class="divItem "><text style="font-weight:bold;">标签：</text><block wx:for="{{labelList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" data-event-params="{{({item,index})}}" class="{{[(true)?'spanItem':'',(activeIndex===index)?'spanActiveSchool':'']}}" bindtap="__e">{{item}}</text></block></view><view data-event-opts="{{[['tap',[['saveAddress',['$event']]]]]}}" class="divSave" bindtap="__e">保存地址</view><block wx:if="{{id}}"><view data-event-opts="{{[['tap',[['deleteAddress',['$event']]]]]}}" class="divDelete" bindtap="__e">删除地址</view></block></view></view></view>